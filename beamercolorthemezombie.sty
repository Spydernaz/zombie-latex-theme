\mode<presentation>

% Custom colours


\definecolor{titleFontColour}{RGB}{176, 36, 24}
\definecolor{subtitleFontColour}{RGB}{0,0,0}
\definecolor{primaryFontColour}{RGB}{102, 96, 89}
\definecolor{secondaryFontColour}{RGB}{174,157,115}
\definecolor{primaryBackgroundColour}{RGB}{255,255,255}
\definecolor{accentColour1}{RGB}{176, 36, 24}


% Set colours to variables

\setbeamercolor{block title}{fg=white, bg=primaryBackgroundColour}
\setbeamercolor{block body}{fg=black, bg=primaryBackgroundColour}
\setbeamercolor{block title alerted}{fg=white, bg=primaryBackgroundColour}
\setbeamercolor{block body alerted}{fg=black, bg=primaryBackgroundColour}
\setbeamercolor{block title example}{fg=white, bg=primaryBackgroundColour}
\setbeamercolor{block body example}{fg=black, bg=primaryBackgroundColour}

% Setting fonts

\setbeamercolor{background canvas}{bg=primaryBackgroundColour}
\setbeamercolor{title}{fg=titleFontColour}
\setbeamercolor{frametitle}{fg=titleFontColour}
\setbeamercolor{framesubtitle}{fg=subtitleFontColour}
\setbeamercolor{normal text}{fg=primaryFontColour}
\setbeamercolor{section in toc}{fg=black}
\setbeamercolor{section in toc shaded}{fg=black}
\setbeamercolor{item}{fg=primaryFontColour}
\setbeamercolor{subitem}{fg=primaryFontColour}
\setbeamercolor{subsubitem}{fg=primaryFontColour}
\setbeamercolor{description item}{fg=primaryFontColour}
\setbeamercolor{caption}{fg=secondaryFontColour}
\setbeamercolor{caption name}{fg=secondaryFontColour}

\mode<all>